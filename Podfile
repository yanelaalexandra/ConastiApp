# Uncomment the next line to define a global platform for your project
# platform :ios, ’11.2’

target 'ConastiAppv1' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ConastiAppv1

  pod ‘Firebase’
  pod 'Firebase/Core'
  pod ‘Firebase/Auth’
  pod ‘Firebase/Database’,’<5’
  pod ‘Firebase/Storage’
  pod 'FBSDKLoginKit'
  pod ‘SDWebImage’
  pod 'Cards'
  pod 'Alamofire' , ' ~> 4.7 '
  pod ‘SwiftyJSON’
end
